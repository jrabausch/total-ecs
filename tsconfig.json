{
  "include": [
    "./src",
    "./spec"
  ],
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "lib": [
      "ES6"
    ],
    "outDir": "./dist",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES6"
  }
}
